# For loops #
For i = 1 To 5
  Print i
End

# Arrays #
nums = [10, 20, 30, 40, 50]
Print nums[0]
Print nums[4]
nums[2] = 99
Print nums[2]
Print Length(nums)

# Push and Pop #
arr = [1, 2, 3]
Push(arr, 4)
Print Length(arr)
Pop(arr)
Print Length(arr)

# Elif #
x = 50
If x < 10
  Print "small"
Elif x < 100
  Print "medium"
Else
  Print "large"
End

# Booleans #
a = True
b = False
Print a
Print b

# Logical operators #
If a And Not b
  Print "logic works"
End

If True Or False
  Print "or works"
End
